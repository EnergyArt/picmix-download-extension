{
    "manifest_version": 3,
    "name": "Picmix - расширение для загрузки Gif изображений!",
    "version": "1.0",
    "description": "",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.picmix.com/*"],
            "js": [
                "build/download.bundle.js",
                "resources/content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": ["*://*.picmix.com/*"],
            "resources": ["resources/button.html"]
        }
    ],
    "permissions": ["downloads"]
}